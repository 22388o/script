{
  "name": "bitcoin-script",
  "description": "Compile Bitcoin Script to JavaScript.",
  "scripts": {
    "compile": "./node_modules/.bin/jison src/script.jison && mv script.js src/script.js",
    "clean": "rm -rf lib",
    "transpile:script": "cp src/script.js lib/script.js",
    "transpile:all": "6to5 src --out-dir lib && npm run transpile:script",
    "transpile:src": "mkdir tmp && cp src/*.js tmp/ && 6to5 tmp --out-dir lib && rm -rf tmp && npm run transpile:script",
    "build:all": "npm run compile && npm run transpile:all",
    "build:src": "npm run compile && npm run transpile:src",
    "test": "npm run build:all && jest lib/",
    "copy": "cp npm-package.json lib/package.json && cp README.md lib/README.md",
    "prepublish": "npm run clean && npm run build:src && npm run copy"
  },
  "dependencies": {
    "big-integer": "^1.3.17",
    "bigi": "^1.2.1",
    "coinkey": "^0.1.0",
    "ecdsa": "^0.6.0",
    "js-beautify": "^1.5.4",
    "ripemd160": "^0.2.0",
    "secure-random": "^1.1.1",
    "sha1": "^1.1.0",
    "sha256": "^0.1.1"
  },
  "devDependencies": {
    "browserify": "^6.2.0",
    "6to5": "^1.15.0",
    "jest-cli": "^0.1.18",
    "jison": "^0.4.15",
    "underscore": "^1.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crm416/script.git"
  },
  "keywords": [
    "bitcoin",
    "javascript",
    "jison"
  ],
  "author": "crmarsh",
  "homepage": "crmarsh.com/script/",
  "version": "0.0.2"
}
